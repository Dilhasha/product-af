{
      "type": "appservermgt",
      "provider": "wso2",
      "serviceGroup": "appservergroup",      
      "host": "mgt.appserver.wso2.com",
      "displayName": "AppServer Manager",
      "description": "AppServer Manager Cartridge",
      "version": "5.2.1",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
      "iaasProvider": [
        {
          "type": "openstack",
          "imageId": "RegionOne/e73ef74a-84c9-4bcc-8249-0f2ea45a8fd2",
          "maxInstanceLimit": "4",
          "property": [
            {
             "name": "instanceType",
             "value": "RegionOne/3"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
         }
      },
      "property": [
         {
            "name": "payload_parameter.SERIVCE_GROUP",
            "value": "appservergroup"
         },
         {
            "name": "payload_parameter.DEPLOYMENT",
            "value": "manager"
         },
         {
            "name": "payload_parameter.MANAGER_SERVICE_TYPE",
            "value": "appservermgt"
         },
         {
            "name": "payload_parameter.WORKER_SERVICE_TYPE",
            "value": "appserverworker"
         },
         {
            "name": "PRIMARY",
            "value": "true"
         },
         {
            "name": "payload_parameter.CLUSTERING",
            "value": "true"
         }
      ]    
}
