{
      "type": "bpsworker",
      "provider": "wso2",
      "serviceGroup": "bpsgroup",
      "host": "bps.wso2.com",
      "displayName": "Business Process Server Worker",
      "description": "BPS Worker Cartridge",
      "version": "3.2.1",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
       "iaasProvider": [
        {
          "type": "openstack",
          "imageId": "REGION/BASE_IMAGE_ID",
          "maxInstanceLimit": "4",    
          "property": [
            {
             "name": "instanceType",
             "value": "REGION/3"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
        }
      },
      "property": [
        {
            "name": "payload_parameter.SERIVCE_GROUP",
            "value": "bpsgroup"
         },
         {
            "name": "payload_parameter.DEPLOYMENT",
            "value": "worker"
         },
         {
            "name": "payload_parameter.MANAGER_SERVICE_TYPE",
            "value": "bpsmgt"
         },
         {
            "name": "payload_parameter.WORKER_SERVICE_TYPE",
            "value": "bpsworker"
         },
         {
            "name": "PRIMARY",
            "value": "true"
         },
         {
            "name": "payload_parameter.CLUSTERING",
            "value": "true"
         }
      ]
}
