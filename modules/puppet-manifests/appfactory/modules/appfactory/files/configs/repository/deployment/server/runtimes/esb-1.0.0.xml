<Runtime>
    <Runtime>esb</Runtime>
    <PAASArtifactStorageURLPattern>{@stage}/esb</PAASArtifactStorageURLPattern>
    <StratosAppIdPrefix>esb</StratosAppIdPrefix>
    <CartridgeAliasPrefix>esb</CartridgeAliasPrefix>
    <CartridgeTypePrefix>gayas</CartridgeTypePrefix>
    <DeploymentPolicy>af-deployment</DeploymentPolicy>
    <AutoscalePolicy>economy</AutoscalePolicy>
    <SubscribeOnDeployment>false</SubscribeOnDeployment>
    <ServerURL>https://esb{stage}.appfactory.private.wso2.com:9443</ServerURL>
    <DevelopmentStageParam>.dev</DevelopmentStageParam>
    <TestingStageParam>.test</TestingStageParam>
</Runtime>
