<Runtime>
    <SupportDataSource>false</SupportDataSource>
    <Runtime>car</Runtime>
    <DeployerClassName>org.wso2.carbon.appfactory.jenkins.deploy.JenkinsCARDeployer</DeployerClassName>
    <PAASArtifactStorageURLPattern>{@stage}/esb</PAASArtifactStorageURLPattern>
    <AliasPrefix>esb</AliasPrefix>
    <CartridgeTypePrefix>esb</CartridgeTypePrefix>
    <DeploymentPolicy>af-deployment</DeploymentPolicy>
    <AutoscalePolicy>economy</AutoscalePolicy>
    <SubscribeOnDeployment>false</SubscribeOnDeployment>
    <UndeployerClassName>org.wso2.carbon.appfactory.jenkins.deploy.JenkinsArtifactUndeployer</UndeployerClassName>
</Runtime>
