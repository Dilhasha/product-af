export GIT_SSL_NO_VERIFY=1
mkdir -p <%= @gitblit_home %>/logs
nohup /opt/java/bin/java -d64 -server ${JAVA_PROXY_CONFIG} -Djava.awt.headless=true -Xms1024m -Xmx4096m -XX:MaxPermSize=1024m  -Djava.io.tmpdir="<%= @base_dir %>/tmp/" -jar gitblit.jar --httpsPort <%= @gitblit_port %> > <%= @gitblit_home %>/logs/git.log 2>&1 &
