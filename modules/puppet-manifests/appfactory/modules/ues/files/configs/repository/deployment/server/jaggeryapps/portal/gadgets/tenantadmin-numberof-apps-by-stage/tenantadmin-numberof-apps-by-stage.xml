<?xml version="1.0" encoding="UTF-8" ?>
    <Module>
    <ModulePrefs title="No of Apps By Type"
                 author="WSO2 Gadget Server"
                 height="230"
                 scrolling="true"
                 tags="charts,pie,visualization"
                 description="A Pie Chart gadget, that takes a json datasource as an input and shows numbers of apps created by Type. It can be configured to fetch data in defined intervals.">
	<Require feature="pubsub-2">
            <Param name="topics">
                <![CDATA[
				<Topic title="timeData" name="timeSliderPub"
				description="Subscribes to time slider" type="object"
				subscribe="true"/>
				]]>
            </Param>
        </Require>
     </ModulePrefs>
    <UserPref name="dataSource"
              display_name="Data Source"
              default_value="../../CXODashboard/dataApis/NoOfAppsByStageForTenant.jag">
    </UserPref>
    <UserPref name="updateGraph"
              display_name="Update Interval (s)"
              default_value="No">
    </UserPref>
    <Content type="html">
    <![CDATA[
 <html>
 <head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<title>Pie Chart</title>
	<link href="../../themes/portal/css/flot-graph.css" rel="stylesheet" type="text/css">
	<link href="../../themes/portal/css/bootstrap.min.css" rel="stylesheet" type="text/css">
	<script language="javascript" type="text/javascript" src="../../js/flot/excanvas.min.js"></script>
	<script language="javascript" type="text/javascript" src="../../js/flot/jquery.js"></script>
	<script language="javascript" type="text/javascript" src="../../js/bootstrap.js"></script>
	<script language="javascript" type="text/javascript" src="../../js/flot/jquery.flot.js"></script>
	<script language="javascript" type="text/javascript" src="../../js/flot/jquery.flot.pie.js"></script>
	<script language="javascript" type="text/javascript" src="../../js/flot/jquery.flot.resize.js"></script>
	<script language="javascript" type="text/javascript" src="js/options-pie.js"></script>
	<script language="javascript" type="text/javascript" src="js/flot-chart-core.js"></script>
</head>

<body>
	<div>
			<div id="placeholder" class="demo-placeholder" style="height:90%"></div>


	</div>
</body>
</html>

    ]]>
</Content>
</Module>


