var config = require('/config.json');

var userModule=require('store').user;
var serverModule = require('store').server;
var user = serverModule.current(session);

if (!user) {
    response.sendRedirect('/CXODashboard/login');
}
else{
 // Check authorization
 if(!user.hasRoles(["admin"] && !user.hasRoles(["cxo"]))) {
	response.sendRedirect('/CXODashboard/login');
 }

}

